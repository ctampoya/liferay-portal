<definition>
	<command name="viewRequestFailedToCompleteAlert">
		<execute function="AssertTextEquals" locator1="Message#ERROR_REQUEST_FAILED_TO_COMPLETE" value1="Your request failed to complete." />
	</command>

	<command name="viewRequiredFieldAlert">
		<execute function="AssertTextEquals#assertPartialText" locator1="TextInput#REQUIRED_ALERT" value1="This field is required." />
	</command>

	<command name="viewSuccess">
		<execute macro-mobile="MobileNavigator#clickRemoveIcon" />

		<execute function="AssertTextEquals" locator1="Message#SUCCESS" value1="Your request completed successfully." />
	</command>
</definition>